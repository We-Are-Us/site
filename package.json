{
  "name": "weareus",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^10.0.0",
    "npm": "^5.6.0"
  },
  "scripts": {
    "compile": "npm run compile:client && npm run compile:server",
    "compile:client": "parcel build -d public -o app.js ./client/index.tsx",
    "postcompile:client": "eslint --no-ignore --config public/.eslintrc public/app.*.js",
    "compile:server": "tsc -p server",
    "start": "env-cmd .env --no-override if-env NODE_ENV=development && npm run start:dev || npm run start:prod",
    "prestart:dev": "npm run compile:server",
    "start:dev": "concurrently --kill-others \"node server.js\" \"parcel -d public -o app.js ./client/index.tsx --log-level 3\"",
    "start:prod": "node server.js"
  },
  "devDependencies": {
    "@types/hapi": "17.0.15",
    "@types/node": "10.5.2",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "bootstrap": "4.1.1",
    "browserslist": "4.0.0",
    "concurrently": "3.6.0",
    "parcel-bundler": "1.9.4",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "dotenv-safe": "6.0.0",
    "env-cmd": "8.0.2",
    "handlebars": "4.0.11",
    "hapi": "17.5.2",
    "if-env": "1.0.4",
    "inert": "5.1.0",
    "vision": "5.3.3",
    "winston": "3.0.0"
  }
}

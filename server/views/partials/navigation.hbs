<div className="my-0 position-relative">
  <div class="position-absolute w-100 text-{{navigationTextColor}}">
    <nav class="navbar navbar-expand-md navbar-light">
      <a class="navbar-brand text-{{navigationTextColor}}" href="/">
        <img src="/public/assets/Logo_White.svg" itemProp="logo" height="32" />
      </a>
      <button
        class="navbar-toggler"
        type="button"
        data-toggle="collapse"
        data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon" />
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto pt-1">
          {{#each headerLinks}}
            <li class="nav-item px-2">
              <a class="{{this.classname}}" href="{{this.href}}">{{this.text}}</a>
            </li>
          {{/each}}
        </ul>
      </div>
    </nav>
  </div>
</div>
